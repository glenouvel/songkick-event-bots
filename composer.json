{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "soundcharts/api-client-bundle": ">=3.6",
        "soundcharts/songkick-api-client-bundle": ">=1.1.4",
        "soundcharts/swarrot-process-manager-bundle": "^1.2",
        "soundcharts/wrapper-bundle": "2.x-dev as 2.0",
        "swarrot/swarrot-bundle": "^1.5",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/runtime": "5.3.*",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "symfony/phpunit-bridge": "^5.3",
        "phpstan/phpstan": "^0.12.88"
    },
    "config": {
        "github-oauth": {
            "github.com": "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"
        },
        "preferred-install": {
            "*": "dist",
            "soundcharts/*": "source"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.soundcharts.com"
        }
    ]
}
